    <connection>RDS</connection>
    <sql>SELECT * FROM 
(SELECT distinct p.patient_id, p.updated_at,'concept_name' concept_name  
  FROM patient p JOIN 
   patient_program pp 
ON p.patient_id = pp.patient_id
 JOIN patient_state ps 
ON pp.patient_program_id = ps.patient_program_id 
WHERE pp.program_id = 1 
AND p.voided = 0 
AND pp.voided = 0  
AND ps.state = 7 
AND ps.voided = 0) as patient_id 

LEFT JOIN

(SELECT patient_id, MIN(start_date) start_date 
FROM patient_state s 
JOIN patient_program p 
ON p.patient_program_id = s.patient_program_id 
WHERE s.voided = 0
   AND s.state = 7 
   AND p.program_id = 1
GROUP BY patient_id) 
   AS start_date 
ON patient_id.patient_id = start_date.patient_id

LEFT JOIN

(SELECT person_id, MIN(DATE(obs_datetime)) as date_enrolled 
FROM obs WHERE
concept_id = 2834 AND value_drug IN (SELECT drug_id
FROM drug d WHERE d.concept_id IN 
(SELECT cs.concept_id FROM concept_set cs WHERE cs.concept_set = 1085))
GROUP BY person_id) as date_enrolled
ON patient_id.patient_id = date_enrolled.person_id

LEFT JOIN 

(SELECT patient_id, re_initiated_check(patient_id, date_enrolled) re_initiated,2280 as transfer_in FROM temp_earliest_start_date
WHERE DATE(date_enrolled) != DATE(earliest_start_date)
GROUP BY patient_id
HAVING re_initiated != 'Re-initiated') AS transfer_in
ON patient_id.patient_id = transfer_in.patient_id

LEFT JOIN

(SELECT patient_id, re_initiated_check(patient_id, date_enrolled) as re_initiated, 2280 as re_initiated_code
  FROM temp_earliest_start_date
  WHERE DATE(date_enrolled) != DATE(earliest_start_date)
  GROUP BY patient_id
  HAVING re_initiated = 'Re-initiated') AS re_initiated
  ON patient_id.patient_id = re_initiated.patient_id

LEFT JOIN

 (SELECT patient_id, age_at_initiation, age_in_days FROM temp_earliest_start_date) ages
ON patient_id.patient_id = ages.patient_id

LEFT JOIN
 
 (SELECT person_id, value_coded who_stage_value_coded, value_text who_stage_value_text FROM obs o2 WHERE concept_id  = 7562 AND o2.voided = 0 GROUP BY person_id) as whostage
 ON patient_id.patient_id = whostage.person_id
 
 LEFT JOIN 
 
 (SELECT person_id, value_coded rfst_value_coded, value_text rfst_value_text FROM obs WHERE concept_id = 7563 
AND voided = 0 GROUP BY person_id)
AS reason_for_starting
ON patient_id.patient_id = reason_for_starting.person_id

LEFT JOIN

(SELECT person_id, DATE(max(value_datetime)) hiv_test_date FROM obs WHERE concept_id = 7882 AND voided = 0
GROUP BY person_id) AS hiv_test_date
ON patient_id.patient_id = hiv_test_date.person_id

LEFT JOIN

(SELECT person_id, COALESCE(MAX(value_text), 'UNKNOWN') test_location FROM obs 
JOIN (SELECT DATE(max(obs_datetime)) max_date from obs WHERE concept_id = 7881 AND voided  = 0 GROUP BY person_id) as max_date
ON DATE(obs.obs_datetime) = max_date.max_date
WHERE obs.concept_id = 7881
GROUP BY person_id) AS hiv_test_location
ON patient_id.patient_id = hiv_test_location.person_id
WHERE updated_at >= '${upd_hiv_staging_info}'





</sql>
