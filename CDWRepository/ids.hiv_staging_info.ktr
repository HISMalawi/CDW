  <step>
    <name>obs - hiv_staging_info</name>
    <type>TableInput</type>
    <description/>
    <distribute>N</distribute>
    <custom_distribution/>
    <copies>1</copies>
    <partitioning>
      <method>none</method>
      <schema_name/>
    </partitioning>
    <connection>RDS</connection>
    <sql>SELECT
	patient_id.patient_id,
	patient_id.updated_at,
	patient_id.concept_name,
	dates_n_ages.date_enrolled,
	dates_n_ages.earliest_start_date,
	coalesce(dates_n_ages.age_at_initiation, ages.age_at_initiation)age_at_initiation,
	dates_n_ages.age_in_days,
	coalesce(transfer_in.re_initiated, re_initiated.re_initiated) re_initiated,
	transfer_in.transfer_in,
	re_initiated.re_initiated_code,
	whostage.who_stage_value_coded,
	whostage.who_stage_value_text,
	reason_for_starting.rfst_value_coded,
	reason_for_starting.rfst_value_text,
	hiv_test_date.hiv_test_date,
	hiv_test_location.test_location
FROM
	(
	SELECT
		distinct p.patient_id,
		p.updated_at,
		'concept_name' concept_name
	FROM
		patient p
	JOIN patient_program pp ON
		p.patient_id = pp.patient_id
	JOIN patient_state ps ON
		pp.patient_program_id = ps.patient_program_id
	JOIN person pr ON
		p.patient_id = pr.person_id
	WHERE
		pp.program_id = 1
		AND p.voided = 0
		AND pp.voided = 0
		AND ps.voided = 0) AS patient_id
LEFT JOIN (
	SELECT
		patient_id,
		date_enrolled,
		earliest_start_date,
		age_at_initiation,
		age_in_days
	FROM
		temp_earliest_start_date) AS dates_n_ages ON
	patient_id.patient_id = dates_n_ages.patient_id
LEFT JOIN (
	SELECT
		DISTINCT
	    patient_id,
		IF(re_initiated_check(patient_id,
		date_enrolled) = 'n/a',
		2441,
		re_initiated_check(patient_id,
		date_enrolled)) re_initiated,
		2280 as transfer_in
	FROM
		temp_earliest_start_date
	WHERE
		DATE(date_enrolled) != DATE(earliest_start_date)
	HAVING
		re_initiated != 'Re-initiated') AS transfer_in ON
	patient_id.patient_id = transfer_in.patient_id
LEFT JOIN (
	SELECT
		DISTINCT
		patient_id,
		IF(re_initiated_check(patient_id,
		date_enrolled) = 'n/a',
		2441,
		re_initiated_check(patient_id,
		date_enrolled)) as re_initiated,
		2280 as re_initiated_code
	FROM
		temp_earliest_start_date
	WHERE
		DATE(date_enrolled) != DATE(earliest_start_date)
	HAVING
		re_initiated = 'Re-initiated') AS re_initiated ON
	patient_id.patient_id = re_initiated.patient_id
LEFT JOIN (
	SELECT
		patient_id,
		age_at_initiation,
		age_in_days
	FROM
		temp_earliest_start_date) ages ON
	patient_id.patient_id.patient_id = ages.patient_id
LEFT JOIN (
	SELECT
		o.person_id,
		  o.who_stage_value_coded,
		  o.who_stage_value_text
	FROM
		(
		SELECT
			o2.obs_id,
			o2.person_id,
			o2.value_coded who_stage_value_coded,
			o2.value_text who_stage_value_text
		FROM
			obs o2
		WHERE
			concept_id = 7562
			AND o2.voided = 0) AS o
	LEFT JOIN 
	(
		SELECT
			obs_id,
			person_id
		FROM
			obs
		WHERE
			concept_id = 7562
			AND voided = 0 ) o3
		ON
		(o.person_id = o3.person_id
			AND o.obs_id &lt; o3.obs_id)
	WHERE
		o3.obs_id IS NULL) as whostage ON
	patient_id.patient_id = whostage.person_id
LEFT JOIN (
	SELECT
		o.person_id,
		o.value_coded rfst_value_coded,
		o.value_text rfst_value_text
	FROM
		(
		SELECT
			obs_id,
			person_id,
			value_coded,
			value_text
		FROM
			obs
		WHERE
			concept_id = 7563
			AND voided = 0) o
	LEFT JOIN (
		SELECT
			obs_id,
			person_id
		FROM
			obs
		WHERE
			concept_id = 7563
			AND voided = 0 ) o2 ON
		(o.person_id = o2.person_id
			AND o.obs_id &lt; o2.obs_id)
	WHERE
		o2.obs_id IS NULL) AS reason_for_starting ON
	patient_id.patient_id = reason_for_starting.person_id
LEFT JOIN (
	SELECT
		person_id,
		DATE(max(value_datetime)) hiv_test_date
	FROM
		obs
	WHERE
		concept_id = 7882
		AND voided = 0
	GROUP BY
		person_id) AS hiv_test_date ON
	patient_id.patient_id = hiv_test_date.person_id
LEFT JOIN (
	SELECT
		person_id,
		COALESCE(MAX(value_text), 'UNKNOWN') test_location
	FROM
		obs
	JOIN (
		SELECT
			DATE(max(obs_datetime)) max_date
		from
			obs
		WHERE
			concept_id = 7881
			AND voided = 0
		GROUP BY
			person_id) as max_date ON
		DATE(obs.obs_datetime) = max_date.max_date
	WHERE
		obs.concept_id = 7881
	GROUP BY
		person_id) AS hiv_test_location ON
	patient_id.patient_id = hiv_test_location.person_id;</sql>
