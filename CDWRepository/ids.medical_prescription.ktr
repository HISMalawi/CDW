    <replace_variables>N</replace_variables>
    <quoteString>N</quoteString>
    <sql>DROP TABLE IF EXISTS pillcount;

CREATE TABLE pillcount SELECT o.order_id,
(COALESCE(SUM(ob.value_numeric),0) + COALESCE(SUM(ob.value_text),0)) pillcount
FROM obs ob
JOIN drug_order do ON ob.order_id = do.order_id
JOIN orders o ON do.order_id = o.order_id
WHERE (ob.concept_id = 2540 OR ob.concept_id IN (SELECT concept_id FROM drug d))
GROUP BY ob.person_id,order_id,do.drug_inventory_id
ORDER BY order_id;

ALTER TABLE pillcount add index(order_id);</sql>
