    <connection>IDS</connection>
    <sql>SELECT encounter_id,
  group_concat(md.openmrs_metadata_id) openmrs_drug_composition,
  group_concat(md.master_definition_id) master_def_drug_composition,
  mp.created_at,
  mp.updated_at 
FROM medication_prescriptions mp
JOIN master_definitions md
ON mp.drug_id = md.master_definition_id
  WHERE mp.updated_at >= '${upd_prescription_has_regimen}'
    AND md.openmrs_metadata_id 
      IN (SELECT drug_id FROM arv_drugs)
    AND md.openmrs_entity_name = 'drug'
GROUP BY encounter_id
           </sql>
