    <sql>SELECT
  o.obs_id,
  o.order_id,
  o.order_id,
  o.order_id,
  o.value_numeric,
  o.value_text,
  o.voided,
  o.voided_by,
  o.date_voided,
  o.void_reason,
  o.date_created,
  o.created_at,
  o.updated_at
FROM obs o
  JOIN encounter e
    ON o.encounter_id = e.encounter_id
  JOIN person p
    ON e.patient_id = p.person_id
  JOIN drug_order do  
	ON o.order_id = do.order_id
WHERE concept_id = 6987 
AND o.updated_at >= '${upd_medicationAdherences}'


